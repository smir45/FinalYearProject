  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "models-post": "npx sequelize-cli model:generate --name Post --attributes title:string,description:string",
    "models-user": "npx sequelize-cli model:generate --name User --attributes name:string,email:string,password:string",
    "models-room": "npx sequelize-cli model:generate --name Room --attributes price:string,catagory:string,beds:integer,ac:boolean,occupied:boolean",
    "models-booking": "npx sequelize-cli model:generate --name Bookings --attributes checkin:date,checkout:date,no_of_person:integer,price:string,status:string",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:status": "npx sequelize-cli db:migrate:status",
    "migrate:rollback": "npx sequelize-cli db:migrate:undo",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "seeder-generate": "npx sequelize seed:generate --name default-root",
    "seed:all": "npx sequelize db:seed:all"
  },
